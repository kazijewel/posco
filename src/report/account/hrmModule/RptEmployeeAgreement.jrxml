					<font fontName="SansSerif" size="11" isBold="false"/>
				</textElement>
				<text><![CDATA[18.1]]></text>
			</staticText>
			<textField>
				<reportElement x="20" y="7" width="515" height="14"/>
				<textElement textAlignment="Justified" verticalAlignment="Top">
					<font fontName="SansSerif" size="11" isBold="true"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["Clause 18 - BONUS SALARY"]]></textFieldExpression>
			</textField>
			<staticText>
				<reportElement x="20" y="65" width="24" height="14"/>
				<box leftPadding="0"/>
				<textElement verticalAlignment="Top">
					<font fontName="SansSerif" size="11" isBold="false"/>
